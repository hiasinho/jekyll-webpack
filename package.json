{
  "name": "jekyll-webpack",
  "version": "1.0.0",
  "main": "webpack.config.js",
  "scripts": {
    "start": "foreman start -f Procfile",
    "deploy": "npm run build && ./bin/deploy",
    "build": "npm run build:webpack && npm run build:jekyll",
    "build:jekyll": "bundle exec jekyll build",
    "build:webpack": "NODE_ENV=production webpack -p",
    "serve:jekyll": "bundle exec jekyll serve --safe --watch --config '_config.yml,_config_dev.yml'",
    "serve:webpack": "./node_modules/.bin/webpack-dev-server --inline --progress --colors"
  },
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "autoprefixer": "^6.7.0",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "imports-loader": "^0.7.0",
    "inuitcss": "git://github.com/inuitcss/inuitcss.git#develop",
    "jquery": "^3.1.1",
    "node-sass": "^4.3.0",
    "open-color": "^1.4.2",
    "postcss-loader": "^1.2.2",
    "sass-loader": "^4.1.1",
    "scrollreveal": "^3.3.4",
    "smoothscroll-polyfill": "^0.3.4",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2"
  }
}
